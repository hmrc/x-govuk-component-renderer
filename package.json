{
  "name": "taasa",
  "version": "0.0.12",
  "description": "Templates as a service",
  "engines": {
    "node": "8"
  },
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --detectOpenHandles --forceExit && npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "nodemon server.js --ignore 'src/dependencies/**/*'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "gray-matter": "^4.0.2",
    "js-yaml": "^3.13.1",
    "marked": "^0.7.0",
    "nunjucks": "^3.2.1",
    "tar": "^6.1.8"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.4",
    "supertest": "^6.1.4"
  },
  "bin": {
    "taasa": "./cli.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./dependencies/*",
      "./node_modules/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
